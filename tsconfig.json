{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@mizchi/luna",
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@mizchi/luna": ["./js/ui"],
      "@mizchi/luna/*": ["./js/ui/*"]
    }
  },
  "include": ["js/**/*.ts", "js/**/*.tsx"]
}
