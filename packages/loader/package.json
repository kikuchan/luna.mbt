{
  "name": "@mizchi/luna-loader",
  "version": "0.1.0",
  "description": "Island loader for Luna SSR hydration",
  "type": "module",
  "main": "loader.js",
  "exports": {
    ".": "./loader.js",
    "./min": "./loader.min.js",
    "./v1": "./ln-loader-v1.js"
  },
  "files": [
    "loader.js",
    "loader.min.js",
    "ln-loader-v1.js"
  ],
  "scripts": {
    "minify": "terser loader.js --module --compress --mangle -o loader.min.js",
    "minify:v1": "terser ln-loader-v1.js --module --compress --mangle -o ln-loader-v1.min.js"
  },
  "keywords": [
    "luna",
    "moonbit",
    "island",
    "ssr",
    "hydration"
  ],
  "author": "mizchi",
  "license": "MIT"
}
